<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">


<html xmlns = "http://www.w3.org/1999/xhtml">
   <head>
      <title>Prospective Student Feedback form</title>
      <link rel="HStaley_Assignment4CSS" href="HStaley_Assignment4.css">
   </head>
   <body>
      <h2>Prospective Student Feedback form</h2>
      <Form>
      	
      	<!--Visitor contact information -->
      	<b>First Name </b><br />
      	</><input type="text" name="firstname" id="firstname" /><br />
      	<b>Last Name </b><br />
      	</><input type="text" name="lastname" id="lastname" /><br />
      	<b>Email Address </b><br />
      	</><input type="text" name="email" id="email" /><br />	
      	<b>Address </b><br />
      	</><input type="text" name="address" id="address" /><br />	
      	<b>City</b><br />
      	</><input type="text" name="city" id="city" /><br />	
      	<b>State</b><br />
      	</><input type="text" name="state" id="state" /><br />
      	<b>Zip Code</b><br />
      	</><input type="text" name="zipcode" id="zipcode" /><br />
      	
      	<!--Favorite things about the campus checkboxes-->
      	<br />
      	<b>Please check your favorite aspects of the campus.</b><br />
      	<input type="checkbox" name="chxAspects" value="Students" /> Students<br />
				<input type="checkbox" name="chxAspects" value="Location"/> Location<br />
				<input type="checkbox" name="chxAspects" value="Campus"/> Campus<br />
				<input type="checkbox" name="chxAspects" Value="Atmosphere"/> Atmosphere<br />
				<input type="checkbox" name="chxAspects" value="Location"/> Location<br />
				<input type="checkbox" name="chxAspects" value="Dorm Rooms"/> Dorm Rooms<br />
				<input type="checkbox" name="chxAspects" value="Sports"/> Sports<br />
				
				<!--University Interest radio buttons-->
				<br />
      	<b>Please select how you became interested in the university.</b><br />
				<input type="radio" name="radInterst" value="Friends" /> Friends<br />
				<input type="radio" name="radInterst" value="Television" /> Television<br />
				<input type="radio" name="radInterst" value="Internet" /> Internet<br />
				<input type="radio" name="radInterst" value="Other" /> Other<br />
				
				<!--Comments area -->
				<br />
				<b>Comments</b><br />
				<textarea name="txtComments"></textarea>
				<br /><br />
				
				<!--Submit and reset buttons -->
				<input type="submit" value="Submit"/><br /><br />
				<input type="reset" value="Reset"/><br />
      </Form>
   </body>
</html>
